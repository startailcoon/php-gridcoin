{
    "name": "coondesign/php-gridcoin",
    "description": "RPC caller written in php for Gridcoin Research",
    "license": "MIT",
    "autoload": {
        "psr-0": {"phpGridcoin": "src/"},
        "exclude-from-classmap": ["/tests/"]
    },
    "authors": [
        {
            "name": "Startail the 'Coon",
            "email": "startail@coondesign.eu",
            "homepage": "http://github.com/coondesign/php-gridcoin",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/coondesign/php-gridcoin/issues"
    },
    "require": {
        "netresearch/jsonmapper": "^4.2.0",
        "curl/curl": "^2.5",
        "php": ">=7.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "ext-xml": "*",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.5 || ~8.0 || ~9.0 || ~10.0",
        "squizlabs/php_codesniffer": "~3.5"
    }
}
